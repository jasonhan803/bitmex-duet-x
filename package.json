  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "main": "main.js",
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve --port 6531",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:6531/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint ngx-admin-demo --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "e2e": "ng e2e",
    "format": "npm run format:ts && npm run format:scss",
    "format:ts": "npm run format:ts:import && prettier --config .prettierrc --write '{e2e,src}/**/*.ts' './main.ts'",
    "format:ts:import": "import-sort --write '{e2e,src}/**/*.ts' './main.ts'",
    "format:scss": "csscomb src",
    "cors": "node cors",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 9001 -s ./.storybook/assets-to-serve",
    "tsc:storybook": "tsc -p src/tsconfig.stories.json"
  },
